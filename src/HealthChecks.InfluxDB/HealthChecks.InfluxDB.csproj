<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;Health;InfluxDB</PackageTags>
    <Description>HealthChecks.InfluxDB is the health check package for InfluxDB.</Description>
    <Version>$(HealthCheckInfluxDB)</Version>
    <PackageId>AspNetCore.HealthChecks.InfluxDB</PackageId>
    <Nullable>enable</Nullable>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="InfluxDB.Client" Version="3.3.0" />
    <PackageReference Include="Microsoft.Bcl.AsyncInterfaces" Version="6.0.0" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.3" />
  </ItemGroup>
</Project>
