<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>$(NetFrameworkVersion);$(NetStandardVersion)</TargetFrameworks>
    <PackageTags>HealthCheck;AWS;Secrets Manager;</PackageTags>
    <Description>HealthChecks.Aws.SecretsManager is the health check package for Secrets Manager.</Description>
    <Version>$(HealthCheckAWSSecretsManager)</Version>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AWSSDK.SecretsManager" Version="3.7.2.33" />
    <PackageReference Include="Microsoft.Extensions.Diagnostics.HealthChecks" Version="6.0.3" />
  </ItemGroup>

</Project>
